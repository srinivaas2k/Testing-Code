package com.testing;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) 
	{
		int n, reverse=0;
		System.out.println("enter the num");
		Scanner s= new Scanner(System.in);
		n=s.nextInt();
		while(n!=0)
		{
			reverse=reverse*10;
			reverse=reverse+n%10;
			n=n/10;
		
		}
			
		System.out.println("reverse num is:"+reverse);
		}
		
		
